{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["lib/collection/index.ts"],
  "project": [
    "**/*.{js,jsx,ts,tsx}",
    "!**/*.scss"
  ],
  "ignore": [
    "utils/auth.ts",
    "load-test.js",
    "styles/plugins/tailwind-motion-spring.ts",
    "styles/plugins/tailwind-elevation/index.ts",
    "tests/e2e/scripts/analyze-test-isolation.ts"
  ],
  "ignoreDependencies": [
    "sharp",
    "esbuild",
    "sass-embedded",
    "luxon",
    "react-hook-form",
    "@tailwindcss/aspect-ratio",
    "@tailwindcss/container-queries",
    "@tailwindcss/typography",
    "@tailwindcss/forms",
    "tailwindcss-animate",
    "@types/luxon",
    "@typescript-eslint/eslint-plugin",
    "@typescript-eslint/parser",
    "eslint-plugin-better-tailwindcss"
  ],
  "ignoreExportsUsedInFile": {
    "type": true
  },
  "ignoreBinaries": ["docker-compose"],
  "playwright": {
    "config": "tests/e2e/playwright.config.ts",
    "entry": [
      "tests/e2e/global-setup.ts",
      "tests/e2e/global-teardown.ts",
      "tests/e2e/suites/**/*.spec.ts"
    ]
  }
}
